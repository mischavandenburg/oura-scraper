# Database Configuration
OURA_DB_HOST=localhost
OURA_DB_PORT=5432
OURA_DB_NAME=health
OURA_DB_USER=health
OURA_DB_PASSWORD=your_database_password

# Oura OAuth2 Credentials
# Get these from https://cloud.ouraring.com/oauth/applications
OURA_CLIENT_ID=your_client_id
OURA_CLIENT_SECRET=your_client_secret

# OAuth2 Tokens
# Run `oura-scraper auth` to get these, or set directly for container deployments
OURA_ACCESS_TOKEN=your_access_token
OURA_REFRESH_TOKEN=your_refresh_token

# Scraper Settings
OURA_SCRAPE_DAYS=7
